{
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true
  },
  "css.format.enable": false,
  "debug.toolBarLocation": "commandCenter",
  "editor.codeActionsOnSave": {
    "source.addMissingImports.ts": "always",
    "source.fixAll": "always",
    "source.fixAll.eslint": "always"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "eslint.codeActionsOnSave.mode": "all",
  "eslint.enable": true,
  "eslint.format.enable": false,
  "eslint.probe": ["javascript", "javascriptreact", "typescript", "typescriptreact"],
  "eslint.run": "onType",
  "eslint.useFlatConfig": true,
  "eslint.validate": ["javascript", "javascriptreact", "typescript", "typescriptreact"],
  "explorer.confirmDragAndDrop": false,
  "files.exclude": {
    "**/.git": true,
    "**/node_modules/**": true,
    "package-lock.json": true
  },
  "files.watcherExclude": {
    "**/.git/**": true,
    "**/.vscode/**": true,
    "**/dist/**": true,
    "**/dll/**": true,
    "**/node_modules/**": true,
    "**/release/**": true,
    "package-lock.json": true
  },
  "html.format.enable": false,
  "javascript.format.enable": false,
  "javascript.suggestionActions.enabled": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "javascript.validate.enable": false,
  "markdown.occurrencesHighlight.enabled": true,
  "npm.exclude": "release/**",
  "prettier.configPath": ".prettierrc.js",
  "scss.completion.completePropertyWithSemicolon": true,
  "scss.completion.triggerPropertyValueCompletion": true,
  "scss.format.enable": false,
  "search.exclude": {
    "**/dist/**": true,
    "**/dll/**": true,
    ".vscode/**": true
  },
  "terminal.integrated.automationProfile.windows": {
    "args": ["-l"],
    "color": "terminal.ansiGreen",
    "env": {
      "NODE_OPTIONS": "-r ts-node/register",
      "PATH": "${env:PATH};${workspaceFolder}\\node_modules\\.bin"
    },
    "icon": "terminal-debian",
    "path": "${env:LOCALAPPDATA}\\Programs\\Git\\bin\\bash.exe"
  },
  "terminal.integrated.defaultProfile.windows": "node-bash-work",
  "terminal.integrated.profiles.windows": {
    "node-bash": {
      "args": ["-l"],
      "env": {
        "PATH": "${env:PATH};${workspaceFolder}\\node_modules\\.bin"
      },
      "path": "${env:PROGRAMFILES}\\Git\\bin\\bash.exe"
    },
    "node-bash-work": {
      "args": ["-l"],
      "env": {
        "PATH": "${env:PATH};${workspaceFolder}\\node_modules\\.bin",
        "PORT": "3000"
      },
      "path": "${env:LOCALAPPDATA}\\Programs\\Git\\bin\\bash.exe"
    }
  },
  "typescript.format.enable": false,
  "typescript.tsdk": "node_modules\\typescript\\lib",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "workbench.iconTheme": "helium-icon-theme",
  "git.ignoreLimitWarning": true
}
